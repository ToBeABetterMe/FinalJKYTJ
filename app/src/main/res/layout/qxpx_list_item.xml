        android:gravity="center"
        android:textSize="24sp" />
    <!-- android:text="性别" -->

    <TextView
        android:id="@+id/tv_pxnr"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="3"
