<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sbackground1"
    android:orientation="vertical" >

    <ScrollView
        android:id="@+id/sl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="10dp" >

                <TextView
                    android:id="@+id/tv_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="姓名*："
                    android:textColor="@color/text_color_red"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_name"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_name"
                    android:background="@drawable/archive_edit_bg"
                    android:enabled="false"
                    android:gravity="center"
                    android:inputType="text"
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tv_grxh"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_name"
                    android:layout_marginLeft="180dp"
                    android:layout_toRightOf="@+id/jkda_et_name"
                    android:text="居民个人Id:"
                    android:textColor="@color/text_color_black"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_personId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_name"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_grxh"
                    android:background="@drawable/archive_edit_bg"
                    android:enabled="false"
                    android:gravity="center"
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="30dp" >

                <TextView
                    android:id="@+id/tv_ytjxh"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="健康档案编号："
                    android:textColor="@color/text_color_black"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_healthFileNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_ytjxh"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_ytjxh"
                    android:background="@drawable/archive_edit_bg"
                    android:ellipsize="middle"
                    android:enabled="false"
                    android:gravity="center"
                    android:maxWidth="50dp"
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tv_xzjg02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/jkda_et_healthFileNumber"
                    android:layout_marginLeft="80dp"
                    android:layout_toRightOf="@+id/jkda_et_healthFileNumber"
                    android:text="居住地代码："
                    android:textColor="@color/text_color_black"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_nowLiveCode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_ytjxh"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_xzjg02"
                    android:background="@drawable/archive_edit_bg"
                    android:enabled="false"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="30dp" >

                <TextView
                    android:id="@+id/tv_xzjg01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="居住地名称："
                    android:textColor="@color/text_color_black"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_nowLiveName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_xzjg01"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_xzjg01"
                    android:background="@drawable/archive_edit_bg"
                    android:enabled="false"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tv_hjdz01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_xzjg01"
                    android:layout_marginLeft="130dp"
                    android:layout_toRightOf="@+id/jkda_et_nowLiveName"
                    android:text="居住地地址*："
                    android:textColor="@color/text_color_black"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_nowLiveAddr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_hjdz01"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_hjdz01"
                    android:background="@drawable/archive_edit_bg"
                    android:enabled="false"
                    android:gravity="center"
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="30dp" >

                <TextView
                    android:id="@+id/tv_ytjxh2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="健康一体机编码："
                    android:textColor="@color/text_color_black"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_machineCode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_ytjxh2"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_ytjxh2"
                    android:background="@drawable/archive_edit_bg"
                    android:ellipsize="middle"
                    android:enabled="false"
                    android:gravity="center"
                    android:maxWidth="50dp"
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" >

                    <requestFocus />
                </EditText>

                <TextView
                    android:id="@+id/tv_xzjg022"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/jkda_et_machineCode"
                    android:layout_marginLeft="80dp"
                    android:layout_toRightOf="@+id/jkda_et_machineCode"
                    android:text="一体机业务序号："
                    android:textColor="@color/text_color_black"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_machineNo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_ytjxh2"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_xzjg022"
                    android:background="@drawable/archive_edit_bg"
                    android:enabled="false"
                    android:gravity="center"
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="30dp" >

                <TextView
                    android:id="@+id/tv_jddw01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="户籍地代码*："
                    android:textColor="@color/text_color_red"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_householdRegisterCode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_jddw01"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_jddw01"
                    android:background="@drawable/archive_edit_bg"
                    android:enabled="false"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tv_jddw02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_jddw01"
                    android:layout_marginLeft="80dp"
                    android:layout_toRightOf="@+id/jkda_et_householdRegisterCode"
                    android:text="户籍地名称："
                    android:textColor="@color/text_color_black"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_householdRegisterName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_jddw01"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_jddw02"
                    android:background="@drawable/archive_edit_bg"
                    android:enabled="false"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="30dp" >

                <TextView
                    android:id="@+id/tv_jddw11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="户籍地地址："
                    android:textColor="@color/text_color_black"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_householdRegisterAddr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_jddw11"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_jddw11"
                    android:background="@drawable/archive_edit_bg"
                    android:enabled="false"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tv_jdsj01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_jddw11"
                    android:layout_marginLeft="80dp"
                    android:layout_toRightOf="@+id/jkda_et_householdRegisterAddr"
                    android:text="建档日期："
                    android:textColor="@color/text_color_black"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_registerDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_jdsj01"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_jdsj01"
                    android:background="@drawable/archive_edit_bg"
                    android:enabled="false"
                    android:gravity="center"
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="30dp" >

                <TextView
                    android:id="@+id/tv_jddw111"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="建档单位代码*："
                    android:textColor="@color/text_color_red"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_registerOrgCode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_jddw111"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_jddw111"
                    android:background="@drawable/archive_edit_bg"
                    android:enabled="false"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tv_jddw222"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_jddw111"
                    android:layout_marginLeft="80dp"
                    android:layout_toRightOf="@+id/jkda_et_registerOrgCode"
                    android:text="建档单位名称："
                    android:textColor="@color/text_color_black"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_registerOrgName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_jddw111"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_jddw222"
                    android:background="@drawable/archive_edit_bg"
                    android:enabled="false"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="30dp" >

                <TextView
                    android:id="@+id/tv_jdr001"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="建档人代码*："
                    android:textColor="@color/text_color_red"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_registerDoctorCode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_jdr001"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_jdr001"
                    android:background="@drawable/archive_edit_bg"
                    android:enabled="false"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tv_jdr002"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_jdr001"
                    android:layout_marginLeft="100dp"
                    android:layout_toRightOf="@+id/jkda_et_registerDoctorCode"
                    android:text="建档人姓名："
                    android:textColor="@color/text_color_black"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_registerDoctorName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_jdr002"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_jdr002"
                    android:background="@drawable/archive_edit_bg"
                    android:enabled="false"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="30dp" >

                <TextView
                    android:id="@+id/tv_ysdm01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="责任医生代码："
                    android:textColor="@color/text_color_black"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_responsibleDoctorCode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_ysdm01"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_ysdm01"
                    android:background="@drawable/archive_edit_bg"
                    android:enabled="false"
                    android:gravity="center"
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tv_ysdm02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_ysdm01"
                    android:layout_marginLeft="90dp"
                    android:layout_toRightOf="@+id/jkda_et_responsibleDoctorCode"
                    android:text="责任医生姓名："
                    android:textColor="@color/text_color_black"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/jkda_et_responsibleDoctorName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/tv_ysdm02"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/tv_ysdm02"
                    android:background="@drawable/archive_edit_bg"
                    android:enabled="false"
                    android:gravity="center"
                    android:textColor="#ff0000ee"
                    android:textSize="20sp" />
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>