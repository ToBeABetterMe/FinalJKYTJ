<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_ggws"
    android:gravity="center"
    android:orientation="vertical" >

    <Button
        android:id="@+id/ggws_btn_child"
        android:layout_width="800dp"
        android:layout_height="100dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/ggws_child_health_btn"
        android:onClick="jumpTo" />

    <Button
        android:id="@+id/ggws_btn_pregnancy"
        android:layout_width="800dp"
        android:layout_height="100dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/ggws_ycf_btn"
        android:onClick="jumpTo" />

</LinearLayout>